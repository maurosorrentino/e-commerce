emId;\n\n        fetch(`${process.env.LOCALHOST}/view-review/${itemId}`, {\n\n            method: 'GET',\n\n            headers: {\n\n                'Accept': 'application/json',\n                'Content-Type': 'application/json',\n\n            },\n\n        })\n\n        .then(res => {\n\n            return res.json()\n\n        })\n\n        .then(resData => {\n\n            // mapping the reviews so that we can show them into the component\n            this.setState({ reviews: resData.reviews.map(review => {\n\n                let reviewRating;\n\n                /* making the number (rating) stars */\n                if(review.rating === 5) {\n\n                    reviewRating = <> <FaStar /> <FaStar /> <FaStar /> <FaStar /> <FaStar /> </> \n\n                }\n\n                if(review.rating === 4) {\n\n                    reviewRating = <> <FaStar /> <FaStar /> <FaStar /> <FaStar /> </> \n\n                }\n\n                if(review.rating === 3) {\n\n                    reviewRating = <> <FaStar /> <FaStar /> <FaStar /> </> \n\n                }\n\n                if(review.rating === 2) {\n\n                    reviewRating = <> <FaStar /> <FaStar /> </> \n\n                }\n\n                if(review.rating === 1) {\n\n                    reviewRating = <> <FaStar /> </> \n\n                }\n\n                return (\n\n                    <ViewReviewStyle key={review._id}>\n                        \n                        <span className=\"orange\">{reviewRating}</span>\n\n                        {/* if the user id is equal to the one that the review has into the db (so this user made the review) we show the remove button */}\n                        {this.props.userId === review.userId && <RemoveReview itemId={this.props.itemId} reviewId={review._id} /> }\n\n                        <h1>{review.text}</h1>\n\n                    </ViewReviewStyle>\n\n                )\n\n            }) })\n\n        })\n\n        .catch(err => console.log